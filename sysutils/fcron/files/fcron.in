#!/bin/sh
#
# $FreeBSD: sysutils/fcron/files/fcron.in 300897 2012-07-14 14:29:18Z beat $
#

# PROVIDE: fcron
# REQUIRE: LOGIN
# BEFORE: securelevel
# KEYWORD: shutdown

. /etc/rc.subr

name="fcron"
rcvar=fcron_enable
command="%%PREFIX%%/sbin/${name}"
pidfile="/var/run/${name}.pid"

load_rc_config $name
run_rc_command "$1"
