#!/bin/sh

# $FreeBSD: ports/sysutils/p5-MogileFS-Server/files/mogstored.in,v 1.3 2012/11/17 06:01:40 svnexp Exp $
#
# PROVIDE: mogstored
# REQUIRE: LOGIN cleanvar
# KEYWORD: shutdown

. /etc/rc.subr

name=mogstored
rcvar=mogstored_enable

load_rc_config mogstored

: ${mogstored_enable:="NO"}

required_files=%%PREFIX%%/etc/mogstored.conf

command_args="--config=%%PREFIX%%/etc/mogstored.conf -d"

run_rc_command "$1"
