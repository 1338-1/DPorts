#!/bin/sh
# $FreeBSD: head/japanese/FreeWnn-server/files/pkg-install.in 340719 2014-01-22 15:52:06Z mat $

case $2 in
POST-INSTALL)
	case $1 in
	ja-FreeWnn-*)
		chown wnn ${PKG_PREFIX}/bin/Wnn4/jserver
		;;
	zh-FreeWnn-*)
		chown wnn ${PKG_PREFIX}/bin/cWnn4/cserver \
		    ${PKG_PREFIX}/bin/cWnn4/tserver
		;;
	ko-FreeWnn-*)
		chown wnn ${PKG_PREFIX}/bin/kWnn4/kserver
	esac
esac
