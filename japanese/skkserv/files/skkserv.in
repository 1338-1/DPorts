#!/bin/sh
# $FreeBSD: japanese/skkserv/files/skkserv.in 335572 2013-12-03 15:05:17Z tijl $
#
# PROVIDE: skkserv
# REQUIRE: LOGIN
# KEYWORD: shutdown
#

. /etc/rc.subr

name=skkserv
rcvar=skkserv_enable
command=%%PREFIX%%/sbin/skkserv

: ${skkserv_enable:="NO"}
: ${skkserv_flags:="-a 127.0.0.1 -u skkserv"}

load_rc_config ${name}
run_rc_command $1
