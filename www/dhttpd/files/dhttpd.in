#!/bin/sh
#
# $FreeBSD: www/dhttpd/files/dhttpd.in 302141 2012-08-05 23:19:36Z dougb $
#
# PROVIDE: dhttpd
# REQUIRE: DAEMON
# KEYWORD: shutdown
#
# Add the following line to /etc/rc.conf to enable dhttpd:
#
# dhttpd_enable (bool):	Set it to "YES" to enable dhttpd
#

. /etc/rc.subr

name="dhttpd"
rcvar=dhttpd_enable

load_rc_config $name

: ${dhttpd_enable="NO"}

command=%%PREFIX%%/sbin/${name}

run_rc_command "$1"
