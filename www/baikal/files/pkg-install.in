#!/bin/sh
# $FreeBSD: head/www/baikal/files/pkg-install.in 308396 2012-12-06 19:27:45Z adamw $

PKG_BATCH=${BATCH:=NO}
PKG_PREFIX=${PREFIX:=/usr/local}

case $1 in
POST-INSTALL)
	if [ "${PKG_BATCH}" = "NO" ]; then
		if [ ! -f %%CALDB%% ]; then
			/bin/cp -p %%CALDB%%-dist %%CALDB%%
		fi
	fi
	;;
esac
