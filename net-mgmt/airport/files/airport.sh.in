#! /bin/sh
#
# $FreeBSD: net-mgmt/airport/files/airport.sh.in 340872 2014-01-24 00:14:07Z mat $

case $0 in
	*airport)
		file=AirportBaseStationConfig
	;;
	*airport2)
		file=Airport2BaseStationConfig
	;;
	*hostmon)
		file=HostMonitor
	;;
	*linkmon)
		file=LinkMonitor
	;;
	*airmodem)
		file=AirportModemUtility
	;;
	*)
	echo "Unknown command"
	exit 1
esac

JAVA_VERSION="%%JAVA_VERSION%%" \
	exec "%%LOCALBASE%%/bin/java" -jar "%%DATADIR%%/${file}.jar" "$@"
