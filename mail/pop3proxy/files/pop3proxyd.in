#!/bin/sh
#
# $FreeBSD: mail/pop3proxy/files/pop3proxyd.in 300896 2012-07-14 13:54:48Z beat $
#

# PROVIDE: pop3proxyd
# REQUIRE: LOGIN
# BEFORE: mail
# KEYWORD: shutdown

#
# Add the following line to /etc/rc.conf to enable pop3proxyd:
#
#pop3proxyd_enable="YES"
#

. /etc/rc.subr

name=pop3proxyd
rcvar=pop3proxyd_enable

command=%%PREFIX%%/sbin/pop3proxyd

load_rc_config $name
run_rc_command "$1"
