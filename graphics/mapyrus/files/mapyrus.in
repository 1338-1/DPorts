#!/bin/sh
# $FreeBSD: ports/graphics/mapyrus/files/mapyrus.in,v 1.7 2012/11/17 05:58:15 svnexp Exp $
if [ -z "${DISPLAY}" ]; then
    JAVA_AWT_HEADLESS="-Djava.awt.headless=true"
else
    JAVA_AWT_HEADLESS=""
fi
if [ -z "$1" ]; then
    MAPYRUS_ARGS="-h"
else
    MAPYRUS_ARGS="$@"
fi
JAVA_VERSION="1.5+" \
	exec "%%LOCALBASE%%/bin/java" ${JAVA_AWT_HEADLESS} -jar "%%DATADIR%%/mapyrus.jar" ${MAPYRUS_ARGS}
